          <!-- Relationship: FeatureReferencesBrother -->
          <xsd:element name="brother" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
              <xsd:documentation>Instance of FeatureReferencesBrother</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
